{
    "name": "endroid/sudoku",
    "description": "Endroid Sudoku solver",
    "keywords": ["endroid", "sudoku", "solver", "php"],
    "homepage": "https://github.com/endroid/sudoku",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "endroid/installer": "^1.0.3"
    },
    "require-dev": {
        "endroid/test": "^1.1.4"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Sudoku\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Sudoku\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
