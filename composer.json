{
    "name": "endroid/sudoku",
    "description": "Endroid Sudoku solver",
    "keywords": ["endroid", "sudoku", "solver"],
    "homepage": "https://github.com/endroid/sudoku",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.9",
        "phpunit/phpunit": "~4.7",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Sudoku\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
