{
    "name": "endroid/sudoku",
    "description": "Endroid Sudoku solver",
    "keywords": ["endroid", "sudoku", "solver"],
    "homepage": "https://github.com/endroid/sudoku",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "symfony/var-dumper": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Sudoku\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Sudoku\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
